			tamsler@cal.berkeley.edu]]>
	</license-agreement>	
	<files>
		<file source="BinaryTree.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="BinaryTreeObject.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="Debug.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="HashTable.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="HashTableObject.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="ListNode.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="ObjectListNode.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="ObjectTreeNode.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="Queue.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="QueueObject.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="SingleLinkedList.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="SingleLinkedListObject.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="Stack.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="StackObject.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
		<file source="TreeNode.as"
		 destination="$Flash/Classes/org/springsoft/aslib" />
	</files>
</macromedia-extension>

	
